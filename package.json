{
  "name": "launchpad-platform",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo dev",
    "test": "turbo run test",
    "test:e2e": "turbo run contracts:deploy-local && pnpm --filter app run test:e2e",
    "start": "turbo run dev",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.15.6"
}
